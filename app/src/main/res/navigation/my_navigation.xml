<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_nav"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/consulationFragment"
        android:name="com.example.nekoshigoto.ConsulationFragment"
        android:label="ConsulationFragment">
        <action
            android:id="@+id/action_consulationFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_consulationFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_consulationFragment_to_activityFragment2"
            app:destination="@id/activityFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_consulationFragment_to_profileFragment2"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.nekoshigoto.HomeFragment"
        android:label="HomeFragment">
        <action
            android:id="@+id/action_homeFragment_to_savedFragment"
            app:destination="@id/savedFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_consulationFragment"
            app:destination="@id/consulationFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
        <action
            android:id="@+id/action_homeFragment_to_activityFragment2"
            app:destination="@id/activityFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
    </fragment>
    <fragment
        android:id="@+id/savedFragment"
        android:name="com.example.nekoshigoto.SavedFragment"
        android:label="SavedFragment">
        <action
            android:id="@+id/action_savedFragment_to_consulationFragment"
            app:destination="@id/consulationFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
        <action
            android:id="@+id/action_savedFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_savedFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
        <action
            android:id="@+id/action_savedFragment_to_activityFragment2"
            app:destination="@id/activityFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.nekoshigoto.ProfileFragment"
        android:label="ProfileFragment">
        <action
            android:id="@+id/action_profileFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_profileFragment_to_savedFragment"
            app:destination="@id/savedFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_profileFragment_to_consulationFragment"
            app:destination="@id/consulationFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_profileFragment_to_activityFragment2"
            app:destination="@id/activityFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
    </fragment>
    <fragment
        android:id="@+id/activityFragment"
        android:name="com.example.nekoshigoto.ActivityFragment"
        android:label="ActivityFragment">
        <action
            android:id="@+id/action_activityFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/fragment_slide_left_enter"
            app:exitAnim="@anim/fragment_slide_right_exit" />
        <action
            android:id="@+id/action_activityFragment_to_savedFragment"
            app:destination="@id/savedFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
        <action
            android:id="@+id/action_activityFragment_to_consulationFragment"
            app:destination="@id/consulationFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
        <action
            android:id="@+id/action_activityFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/fragment_slide_right_enter"
            app:exitAnim="@anim/fragment_slide_left_exit" />
    </fragment>
</navigation>